#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--){
	    int D;      //number of digits in number
	    string N;   //the number N entered as a string
	    cin>>D;
	    cin>>N;
	    int flag = 0;
	   for(int i=0; i<D; i++){
	       if(N[i]=='0'||N[i]=='5'){
	           flag=1;
	           break;
	       }
	   }
	   if(flag==1){
	           cout<<"Yes"<<endl;
	       }
	       else{
	           cout<<"No"<<endl;
	       }
	}
	return 0;
}
